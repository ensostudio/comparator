{
  "name": "ensostudio/comparator",
  "description": "The flexible comparation of any PHP variables",
  "keywords": ["compare", "comparation", "comparator", "php"],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/ensostudio/comparator",
  "authors": [
    {
      "name": "Anton Fedonyuk",
      "email": "info@ensostudio.ru",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.25"
  },
  "autoload": {
    "psr-4": {
      "EnsoStudio\\Comparator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EnsoStudio\\Comparator\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
